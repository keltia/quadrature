               <table:table-cell table:style-name="Table154.A1" office:value-type="string">
                  <text:p text:style-name="P15">For electronic communications the following specific principles apply:</text:p>
               </table:table-cell>
            </table:table-row>
            <table:table-row table:style-name="Table154.1">
               <table:table-cell table:style-name="Table154.A1" office:value-type="string">
